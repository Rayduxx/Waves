                  <ImageView fx:id="imageL" fitHeight="98.0" fitWidth="123.0" pickOnBounds="true" />
               </children>
               <padding>
                  <Insets top="5.0" />
               </padding>
            </HBox>
            <Separator minHeight="11.0" prefHeight="54.0" prefWidth="350.0" />
            <Label fx:id="artisteL" alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" prefWidth="350.0" text="Label" textAlignment="CENTER" wrapText="true">
               <padding>
                  <Insets top="4.0" />
               </padding>
            </Label>
            <Label fx:id="genreL" alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" prefWidth="350.0" text="Label" textAlignment="CENTER" wrapText="true">

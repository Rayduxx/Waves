                     <graphic>
                        <ImageView fitHeight="16.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@assets/icons8-comment-50.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
            <Separator minHeight="11.0" prefHeight="54.0" prefWidth="350.0" />
            <ScrollPane prefHeight="373.0" prefWidth="350.0">
               <content>
                  <VBox fx:id="Box" prefHeight="60.0" prefWidth="348.0" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>

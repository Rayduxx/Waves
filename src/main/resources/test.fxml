               </padding>
            </Label>
            <Separator minHeight="11.0" prefHeight="54.0" prefWidth="350.0" />
            <HBox alignment="CENTER" prefHeight="27.0" prefWidth="291.0" spacing="4.0">
               <children>
                  <TextField fx:id="AJ" prefHeight="27.0" prefWidth="303.0" />

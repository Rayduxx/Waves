<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchp" prefHeight="500.0" prefWidth="350.0" style="-fx-border-color: #310047;" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controllers.CardController">
   <children>
      <ImageView fitHeight="500.0" fitWidth="350.0" opacity="0.54" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@assets/cardcom.png" />
         </image>
      </ImageView>
      <VBox fx:id="vbox" alignment="CENTER" prefHeight="500.0" prefWidth="350.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="titreL" alignment="CENTER" prefHeight="20.0" prefWidth="461.0" stylesheets="@css/style.css" text="Label" textAlignment="JUSTIFY" textOverrun="CLIP">
